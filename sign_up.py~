    def sum(self):
            a = self.plainTextEdit_2.toPlainText()
            b = self.plainTextEdit_3.toPlainText()

            try:
                c = int(a)+int(b)
                self.lcdNumber.display(c)
            except:
                print("Invalid Inputs")

    def multiply(self):
            a = self.plainTextEdit_2.toPlainText()
            b = self.plainTextEdit_3.toPlainText()

            try:
                c = int(a)*int(b)
                self.lcdNumber.display(c)
            except:
                print("Invalid Inputs")



        
